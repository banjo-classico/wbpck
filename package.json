  "description": "Test for webpack 3",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "webpack",
    "start": "better-npm-run start",
    "build": "better-npm-run build",
    "start:server:watch": "nodemon ./node_modules/.bin/babel-node server"
  },
  "betterScripts": {
    "start": {
      "command": "webpack-dev-server --host 0.0.0.0 --colors --hot --inline --watch-poll --config webpack.dev.config.js",
      "env": {
        "TARGET": "http://localhost:3000",
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --config webpack.prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
